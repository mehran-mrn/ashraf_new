{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WEBDeon/BlogEtc"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "anetwork/validation": "^2.2",
        "askedio/laravel-soft-cascade": "^5.8",
        "atayahmet/laravel-nestable": "^0.8.9",
        "barryvdh/laravel-translation-manager": "^0.5.3",
        "econea/nusoap": "^0.9.6",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "kim/activity": "^1.1",
        "larabook/gateway": "^3.3",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.3",
        "santigarcor/laratrust": "5.0.*",
        "unisharp/laravel-filemanager": "v2.0.0-alpha8",
        "webdevetc/blogetc": "dev-patch-1"
    },

    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "files": ["app/Http/helpers.php","app/Http/jdf.php"],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php vendor/bin/homestead make",
            "composer keys",
            "composer meta"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "php artisan key:generate"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "meta": [
            "php artisan optimize"
        ],
        "keys": [
            "php artisan key:generate"
        ]
    }
}
